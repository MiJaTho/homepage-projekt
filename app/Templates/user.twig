{% extends "layout.twig" %}
{% set act = "users" %}
{% block title %}User{% endblock %}

{% block content %}

<div class="container">

  {% if logged_in %}
              
     <table class="table-striped">
        
       
        <thead>
         <tr> <th> User </th> </tr> 
         </thead> 
         <tbody>
         {% for user in users %}
         <tr> <td>{{ user.username }}</td>
               <td>{{ user.screenname }}</td> 
        </tr> 
          {% endfor %}
          </tbody>
          
          </table>
       
        
           {% else %}
        <h2>Place LOGIN!</h2>
  {% endif %}


   </div>

{% endblock %}
